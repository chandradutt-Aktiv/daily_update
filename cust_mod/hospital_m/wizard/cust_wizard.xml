<odoo>
    <data>
        <record model="ir.ui.view" id="chandradutt_wizard_form">
          <field name="name">chandradutt_wizard form</field>
          <field name="model">chandradutt_wizard</field>
          <field name="arch" type="xml">
            <form string="abc">
              <field name="fname" use="required"/>
              <field name="lname" widget="name" use="required"/>
              <field name="mobile" />
              <field name="email" widget="email"/>
                
            </form>
          </field>
        </record>
    </data>
</odoo>