<odoo>
    <data>
        <record model="ir.ui.view" id="travel_management_form">
          <field name="name">travel_management form</field>
          <field name="model">travel_management.wizard</field>
          <field name="arch" type="xml">
            <form string="abc">
                <group>
                  <field name="id" use="required"/>
                  <field name="name" widget="name" use="required"/>
                  <field name="dob" />
                  <field name="email" widget="email"/>
                </group>
                <footer>
                    <button name="%(travel_management.action_window)d" string="Print" type="action"/>
                    <button name="cancel" string="Cancel" type="cancel"/>
                </footer>

        <!--          <field name="city" />-->
        <!--          <field name="boarding"/>-->
        <!--          <field name="destination"/>-->
        <!--          <field name="gender" options="{'horizontal':true}"/>-->
            </form>
          </field>
        </record>

        <record model="ir.actions.act_window" id="travel_management_wiz_action_window">
            <field name="name">travel_management form</field>
            <field name="res_model">travel_management.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="travel_management_form"/>

            <field name="target">new</field>
        </record>


    </data>
</odoo>
